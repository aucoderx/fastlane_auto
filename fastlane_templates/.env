 PROJECT_NAME=app-demo
 
 # 应用名称，用于构建文件命名和显示
APP_NAME=genapp

# iOS Bundle Identifier，格式：com.company.appname
# 必须与 Apple Developer 中注册的 App ID 一致
BUNDLE_ID=com.anonymous.genapp

# Android 包名，格式：com.company.appname
# 必须与 Google Play Console 中注册的包名一致
ANDROID_PACKAGE_NAME=com.anonymous.genapp

# Apple 相关配置
# Apple ID（开发者账号邮箱）
APPLE_ID=contact@autocoder.cc
TEAM_ID='AGGV773Z7Z'
APP_STORE_CONNECT_API_KEY_ID=55A9Z23V8S
APP_STORE_CONNECT_ISSUER_ID=5826677d-1ee5-4ec2-9008-d053564086bb
APP_STORE_CONNECT_API_KEY_PATH=fastlane/certificates/AuthKey_55A9Z23V8S.p8

# Google Play 相关配置
# Google Play Console 服务账号 JSON 密钥文件路径
GOOGLE_PLAY_JSON_KEY_PATH=fastlane/certificates/google-play-service-account.json

# 项目路径配置
# Xcode Workspace 文件路径，若使用fastlane打包，会用到
WORKSPACE_PATH=genapp.xcworkspace
# 用于指定构建的 scheme，默认值：${APP_NAME}
SCHEME_NAME=genapp
# Xcode 项目文件路径，用于版本号管理等操作，默认值：${APP_NAME}.xcodeproj
XCODEPROJ_PATH=genapp.xcodeproj
# Android 项目根目录
ANDROID_PROJECT_DIR="/Users/xiexingyun/workspace/fastlane/app-demo/android"

# 应用元数据
APP_DESCRIPTION_EN="test app"
APP_SHORT_DESCRIPTION_EN="test app"
APP_DESCRIPTION=测试用移动应用
APP_SHORT_DESCRIPTION=测试用移动应用
APP_KEYWORDS=app,mobile
APP_CATEGORY=PRODUCTIVITY
APP_SECONDARY_CATEGORY=BUSINESS
APP_VERSION=105
PRIVACY_URL=https://www.autocoder.cc/privacy

# 发布选项
SUBMIT_FOR_REVIEW=false
AUTOMATIC_RELEASE=false
AUTO_SUBMIT=false

# 截图配置
SCREENGRAB_CLEAR_PREVIOUS=true
SCREENGRAB_OUTPUT_DIRECTORY=./fastlane/metadata/android
